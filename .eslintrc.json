{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "object-curly-spacing": [
            2,
            "always"
        ],
        "react/react-in-jsx-scope": "off",
        "indent": [
            "error",
            2,
            {
                "SwitchCase": 1
            }
        ],
        "comma-dangle": [
            "error",
            {
                "functions": "never"
            }
        ],
        "require-jsdoc": 0,
        "react/prop-types": 0,
        "no-unused-vars": "warn",
        "max-len": "off"
    }
}